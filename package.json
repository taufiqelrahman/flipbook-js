{
  "name": "flipbook-js",
  "version": "1.1.0",
  "description": "A plugin built on javascript which allows users animate to a book with flippable pages",
  "main": "dist/flipbook.esm.min.js",
  "module": "dist/flipbook.esm.min.js",
  "types": "dist/flipbook.d.ts",
  "type": "module",
  "author": "Taufiq",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/flipbook.esm.min.js",
      "require": "./dist/flipbook.umd.min.js"
    },
    "./style.css": "./dist/flipbook.min.css"
  },
  "scripts": {
    "scss": "sass src/style.scss dist/flipbook.css",
    "lint": "eslint --fix src",
    "build": "yarn lint && yarn scss && rollup -c && cp src/flipbook.d.ts dist/flipbook.d.ts",
    "minify": "cleancss -o dist/flipbook.min.css dist/flipbook.css && terser dist/flipbook.umd.js -o dist/flipbook.umd.min.js --compress --mangle && terser dist/flipbook.esm.js -o dist/flipbook.esm.min.js --compress --mangle",
    "demo": "cp dist/flipbook.umd.min.js demo/flipbook.umd.min.js && cp dist/flipbook.min.css demo/flipbook.min.css"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "clean-css-cli": "^5.6.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.5",
    "rollup": "^4.46.3",
    "sass": "^1.90.0",
    "terser": "^5.43.1"
  },
  "keywords": [
    "flipbook",
    "flippable",
    "pages",
    "animation",
    "javascript",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taufiqelrahman/flipbook-js.git"
  },
  "bugs": {
    "url": "https://github.com/taufiqelrahman/flipbook-js/issues"
  },
  "homepage": "https://flipbook-js.vercel.app",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/flipbook.umd.min.js",
    "dist/flipbook.esm.min.js",
    "dist/flipbook.min.css",
    "dist/flipbook.d.ts"
  ]
}
